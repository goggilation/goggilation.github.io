{
  "source": ["src/tokens/transformed/**/*.json"],
  "platforms": {
    "scss": {
      "transformGroup": "css",
      "buildPath": "src/styles/tokes/css/",
      "transforms": ["attribute/cti", "name/cti/kebab", "time/seconds", "content/icon", "size/rem", "color/hsl"],
      "files": [{
        "destination": "_variables.css",
        "format": "css/variables"
      }]
    }
  }
}
